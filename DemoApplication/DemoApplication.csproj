  <ItemGroup>
    <ProjectReference Include="..\DeviceIOControlLib\DeviceIOControlLib.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing" Version="1.0.0-beta004" />
  </ItemGroup>

</Project>
